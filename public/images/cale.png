<script type="text/javascript">
$('document').ready(function(){
      var currentDate = new Date();

      $('#date').datepicker({ 
          maxDate: new Date,
          inline: true,
          showOtherMonths: true, 
          dayNamesMin: ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'], 
          dateFormat: 'dd/mm/yy',
          
      }); 

      $("#date").datepicker("setDate", currentDate); 

      $('#msg').delay(2000).fadeOut();
 
      $('#checkin').datetimepicker({
        pickDate: false
      });

      $('#checkout').datetimepicker({
        pickDate: false
      });

      $('#emp_name').change(function(){
           var val = $(this).val();
           if(val == 0)
           {
               $('#emp_name_msg').text("Please Select Employee Name");
           }
           else
           {  
              $('#emp_name_msg').text(" ");  
           }
      });

      $('#checkin_btn').click(function(){
           var val1 = $('#checkin1').val();
           $('#checkin_msg').text(" ");  
      });
    
    $('#checkin1').blur(function(){
       var val1 = $('#checkin1').val();
       if(val1.length == 0)
       {
          $('#checkin_msg').text("Please select checkin time"); 
       }
       else
       {
          $('#checkin_msg').text(" ");   
       }
     });
      
    $('#checkout_btn').click(function(){
           var val1 = $('#checkout1').val();
           $('#checkout_msg').text(" ");  
      });
    
    $('#checkout1').blur(function(){
       var val1 = $('#checkout1').val();
       if(val1.length == 0)
       {
          $('#checkout_msg').text("Please select checkin time"); 
       }
       else
       {
          $('#checkout_msg').text(" ");   
       }
     });
});
</script>



